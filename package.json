{
  "name": "California Miramar History",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-lowe-nz/California Miramar History"
  },
  "author": "michael-lowe-nz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michael-lowe-nz/California Miramar History/issues"
  },
  "homepage": "https://github.com/michael-lowe-nz/California Miramar History",
  "dependencies": {
    "clone": "^2.1.0",
    "deep-freeze": "0.0.1",
    "radium": "^0.18.1",
    "react-animations": "^0.1.0",
    "redux": "^3.6.0",
    "sheet-router": "^4.2.1",
    "superagent": "^3.4.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
